<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-sdk tools:overrideLibrary="com.google.zxing.client.android" />

    <application
        android:hardwareAccelerated="true"
        android:name="com.example.inventory.InventoryApplication"
        android:allowBackup="true"
        android:icon="@drawable/isic_logo"
        android:label="MyPointOfSale"
        android:roundIcon="@drawable/isic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.InventoryApp">
        <activity
            android:exported="true"
            android:name="com.example.inventory.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>